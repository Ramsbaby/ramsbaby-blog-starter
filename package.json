    "start": "npm run develop",
    "start-global": "gatsby develop --host=0.0.0.0",
    "build": "gatsby build",
    "lh": "lighthousebot",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier -c \"src/**/*.{js,jsx,ts,tsx,md,scss}\"",
